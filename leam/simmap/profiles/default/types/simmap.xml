<?xml version="1.0"?>
<object name="simmap"
   meta_type="Factory-based Type Information with dynamic views"
   i18n:domain="leam.simmap" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <property name="title" i18n:translate="">simmap</property>
  <property name="description"
    i18n:translate="">SimMap version 2</property>
  <property name="content_meta_type">simmap</property>
  <property name="content_icon">document_icon.gif</property>
  <property name="product">leam.simmap</property>
  <property name="factory">addsimmap</property>
  <property name="immediate_view">atct_edit</property>
  <property name="global_allow">True</property>
  <property name="filter_content_types">False</property>
  <property name="allow_discussion">True</property>
  <property name="default_view">standard_view</property>
  <property name="view_methods">
    <element value="standard_view" />
    <element value="another_view" />
    <element value="base_view" />
  </property>
  <alias from="(Default)" to="(dynamic view)" />
  <alias from="edit" to="atct_edit" />
  <alias from="sharing" to="@@sharing" />
  <alias from="view" to="(selected layout)" />
  <action title="Standard SimMap View" action_id="standard_view" category="object" condition_expr=""
    url_expr="string:${object_url}/standard_view" visible="True">
    <permission value="View" />
  </action>
  <action title="Another SimMap View" action_id="another_view" category="object" condition_expr=""
    url_expr="string:${object_url}/another_view" visible="True">
    <permission value="View" />
  </action>
  <action title="Old view" action_id="third_view" category="object" condition_expr=""
    url_expr="string:${object_url}/third_view" visible="True">
    <permission value="View" />
  </action>
  <action title="Raw data view" action_id="base_view" category="object" condition_expr=""
    url_expr="string:${object_url}/base_view" visible="True">
   <permission value="View" />
  </action>
  <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
    <permission value="Modify portal content" />
  </action>
</object>
