    <script type="text/javascript">
        var map;
        window.onload = function() {
            if (GBrowserIsCompatible()) {
                var divmap = document.getElementById('map_canvas');
                map = new GMap2(divmap);
                map.setCenter(new GLatLng(40.11254201209941, -88.24356079101562), 11);
                currentmapcontrol=new GLargeMapControl();
                map.addControl(currentmapcontrol);

            }
        }
    </script>

</head>

